{
  "name": "calendar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "concat": "^1.0.3",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "2.0.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "node-sass src/sass/main.scss public/style.comp.css",
    "concat:css": "concat -o public/style.concat.css public/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/style.concat.css -o public/style.prefix.css",
    "compress:css": "node-sass public/style.prefix.css public/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
