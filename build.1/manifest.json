{
  "name": "Countdown Calendar and Planner",
  "version": "1.3",
  "description": "Countdown to certain events, make plans, and organize your schedule!",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/calendar16.png",
      "32": "images/calendar32.png",
      "48": "images/calendar48.png",
      "128": "images/calendar128.png"
    }
  },
  "icons": 
    {
      "16": "images/calendar16.png",
      "32": "images/calendar32.png",
      "48": "images/calendar48.png",
      "128": "images/calendar128.png"
    },
  "manifest_version": 2
}
